    // Show/hide "Back to Top" button
    function initializeBackToTop() {
      const backToTopBtn = document.getElementById('back-to-top');
      window.addEventListener('scroll', () => {
        if (window.scrollY > 300) {
          backToTopBtn.classList.remove('hidden');
        } else {
          backToTopBtn.classList.add('hidden');
        }
      });
    }

    // Instead of partial-based loading, just init on DOMContentLoaded
    document.addEventListener('DOMContentLoaded', () => {
      initializeBackToTop();
    });

    const navLeft = document.querySelector('.nav-left');
const navLinks = document.querySelectorAll('.nav-left .nav-link');
const menuIndicator = document.getElementById('menu-indicator');

navLinks.forEach(link => {
  link.addEventListener('mouseenter', () => {
    const linkRect = link.getBoundingClientRect();
    const navLeftRect = navLeft.getBoundingClientRect();

    // Only move the indicator, NEVER touch .nav-link styles
    menuIndicator.style.width = `${linkRect.width}px`;
    menuIndicator.style.left = `${linkRect.left - navLeftRect.left}px`;
    menuIndicator.style.opacity = '1';
  });

  link.addEventListener('mouseleave', () => {
    menuIndicator.style.opacity = '0';
  });
});




    /*
      PART 2: The frosted-glass-header logic (from frosted-glass-header.html).
      We renamed the main container #header -> #frosted-header, so be mindful.
    */
    const HEADER_START_HEIGHT = 120;
    const HEADER_END_HEIGHT = 50;
    const SCROLL_RANGE = 150; // Increased for smoother transition

    const frostedHeader = document.getElementById('frosted-header');
    const logoContainer = document.getElementById('logo-container');
    const logoCircle = document.getElementById('logo-circle');
    const logoImg = document.getElementById('logo');
    const brandText = document.getElementById('brand-text');
    const backToTop = document.getElementById('back-to-top');

    function onScroll() {
      // Skip ALL animations on mobile devices
      if (window.innerWidth <= 768) {
        frostedHeader.style.transition = 'none';
        logoContainer.style.transition = 'none';
        logoCircle.style.transition = 'none';
        logoImg.style.transition = 'none';
        brandText.style.transition = 'none';
        
        frostedHeader.style.height = '';
        logoCircle.style.width = '';
        logoCircle.style.height = '';
        logoContainer.style.transform = 'translate(-50%, -50%)';
        brandText.style.transform = '';
        brandText.style.opacity = '0';
        brandText.style.visibility = 'hidden';
        
        return;
      }
      
      const scrollY = window.scrollY;
      const progress = Math.min(scrollY / SCROLL_RANGE, 1);

      // Use cubic-bezier easing for smoother animation
      const easeProgress = progress < 0.5
        ? 2 * progress * progress
        : 1 - Math.pow(-2 * progress + 2, 2) / 2;

      // Shrink header height
      const newHeight = HEADER_START_HEIGHT - (HEADER_START_HEIGHT - HEADER_END_HEIGHT) * easeProgress;
      frostedHeader.style.height = newHeight + "px";

      // Logo circle scales
      const logoSize = HEADER_START_HEIGHT - (HEADER_START_HEIGHT - HEADER_END_HEIGHT) * easeProgress;
      logoCircle.style.width = logoSize + "px";
      logoCircle.style.height = logoSize + "px";

      // Move logo container to the right on scroll
      const logoMove = easeProgress * 80; // Positive value to move right
      logoContainer.style.transform = `translate(${logoMove}px, -50%)`;

      // Show brand text when scrolling
      if (easeProgress > 0.2) {
        // Calculate opacity
        const opacityProgress = Math.min(1, (easeProgress - 0.2) / 0.3);
        
        // Position text to left of logo
        brandText.style.opacity = opacityProgress;
        brandText.style.visibility = 'visible';
        brandText.style.transform = 'translate(-20px, -50%)';
      } else {
        brandText.style.opacity = '0';
        brandText.style.visibility = 'hidden';
      }
    }

    window.addEventListener('scroll', onScroll);
    onScroll(); // run once on load

    function scrollToTop() {
      window.scrollTo({ top: 0, behavior: 'smooth' });
    }

    // Handle mobile header scrolling
    function handleMobileHeaderScroll() {
      const frostedHeader = document.getElementById('frosted-header');
      const logoContainer = document.getElementById('logo-container');
      const mobileNavLeft = document.querySelector('.mobile-nav-left');
      const mobileNavRight = document.querySelector('.mobile-nav-right');
      const brandText = document.getElementById('brand-text');
      
      if (window.innerWidth <= 768) {
        if (window.scrollY > 20) { // Reduced threshold for quicker response
          frostedHeader.classList.add('scrolled');
          
          // Keep mobile navigation visible
          if (mobileNavLeft && mobileNavRight) {
            mobileNavLeft.style.opacity = '1';
            mobileNavLeft.style.visibility = 'visible';
            mobileNavRight.style.opacity = '1';
            mobileNavRight.style.visibility = 'visible';
          }
          
          // Keep logo visible
          if (logoContainer) {
            logoContainer.style.opacity = '1';
            logoContainer.style.visibility = 'visible';
            logoContainer.style.pointerEvents = 'auto';
          }
          
          // Handle brand text visibility on mobile
          if (brandText) {
            brandText.style.opacity = '0'; // Keep hidden on mobile
            brandText.style.visibility = 'hidden';
          }
        } else {
          frostedHeader.classList.remove('scrolled');
          
          // Keep mobile navigation visible
          if (mobileNavLeft && mobileNavRight) {
            mobileNavLeft.style.opacity = '1';
            mobileNavLeft.style.visibility = 'visible';
            mobileNavRight.style.opacity = '1';
            mobileNavRight.style.visibility = 'visible';
          }
          
          // Keep logo visible
          if (logoContainer) {
            logoContainer.style.opacity = '1';
            logoContainer.style.visibility = 'visible';
            logoContainer.style.pointerEvents = 'auto';
          }
          
          // Handle brand text visibility on mobile
          if (brandText) {
            brandText.style.opacity = '0'; // Keep hidden on mobile
            brandText.style.visibility = 'hidden';
          }
        }
      } else {
        // Remove scrolled class on desktop regardless of scroll position
        frostedHeader.classList.remove('scrolled');
        
        // Hide mobile navigation on desktop
        if (mobileNavLeft && mobileNavRight) {
          mobileNavLeft.style.opacity = '0';
          mobileNavLeft.style.visibility = 'hidden';
          mobileNavRight.style.opacity = '0';
          mobileNavRight.style.visibility = 'hidden';
        }
        
        // Always show logo on desktop
        if (logoContainer) {
          logoContainer.style.opacity = '1';
          logoContainer.style.visibility = 'visible';
          logoContainer.style.pointerEvents = 'auto';
        }
      }
    }
    
    window.addEventListener('scroll', handleMobileHeaderScroll);
    window.addEventListener('resize', handleMobileHeaderScroll);
    handleMobileHeaderScroll(); // Run once on page load

    // Function to disable loading animations on mobile
    function optimizeMobile() {
      // Check if device is mobile
      if (window.innerWidth <= 768) {
        // Simplify logo behavior
        const logo = document.getElementById('logo');
        const logoCircle = document.getElementById('logo-circle');
        if (logo && logoCircle) {
          logo.style.transition = 'none';
          logoCircle.style.transition = 'none';
        }
      }
    }

    // Run optimization on load and resize
    window.addEventListener('load', optimizeMobile);
