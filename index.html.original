<script>
document.addEventListener('scroll', () => {
  const progressContainer = document.getElementById('progress-container');
  const progressBar = document.getElementById('progress-bar');
  const headerShrinkThreshold = 100;
  const totalScroll = document.body.scrollHeight - window.innerHeight;
  const sections = [
    { id: 'jaunumi', link: document.getElementById('link-jaunumi') },
    { id: 'contact-section', link: document.getElementById('link-sazinies') },
    { id: 'parbiedribu', link: document.getElementById('link-parbiedribu') }
  ];

  if (window.scrollY > headerShrinkThreshold) {
    progressContainer.classList.remove('hidden');
  } else {
    progressContainer.classList.add('hidden');
  }

  let scrollPercentage = (window.scrollY / totalScroll) * 100;
  progressBar.style.transition = 'width 0.2s ease-out';
  progressBar.style.width = scrollPercentage + '%';

  // Highlight the active section button
  let activeSection = null;
  sections.forEach(section => {
    const sectionElement = document.getElementById(section.id);
    if (sectionElement && sectionElement.getBoundingClientRect().top <= window.innerHeight * 0.6) {
      activeSection = section;
    }
  });

  sections.forEach(section => section.link.classList.remove('active'));
  if (activeSection) {
    activeSection.link.classList.add('active');
    setTimeout(() => {
      if (activeSection.id === 'contact-section') {
        activeSection.link.classList.add('active');
      }
    }, 300);
  }
});

// Add smooth scrolling for navigation links
document.addEventListener('DOMContentLoaded', () => {
  // Get all navigation links that should scroll to sections
  const navLinks = document.querySelectorAll('.nav-left .nav-link');
  console.log("Found nav links:", navLinks.length);
  
  // Add both event listener and onclick handler for redundancy
  navLinks.forEach(link => {
    // The standard event listener approach
    link.addEventListener('click', handleNavClick);
    
    // Also add direct onclick attribute as a backup
    link.onclick = handleNavClick;
    
    // Add visual feedback on hover to show links are active
    link.addEventListener('mouseover', () => {
      link.style.transform = "scale(1.05)";
    });
    
    link.addEventListener('mouseout', () => {
      link.style.transform = "";
    });
  });
  
  // Centralized click handler function
  function handleNavClick(e) {
    e.preventDefault(); // Prevent default anchor behavior
    console.log("Nav link clicked:", e.target.getAttribute('href'));
    
    // Get the target section id from the href attribute
    const targetId = e.target.getAttribute('href').substring(1);
    const targetSection = document.getElementById(targetId);
    
    if (targetSection) {
      console.log("Scrolling to section:", targetId);
      
      // Calculate offset to account for the fixed header
      const headerHeight = document.getElementById('frosted-header').offsetHeight;
      const targetPosition = targetSection.getBoundingClientRect().top + window.pageYOffset - headerHeight - 20;
      
      // Smooth scroll to the target section
      window.scrollTo({
        top: targetPosition,
        behavior: 'smooth'
      });
    } else {
      console.warn("Target section not found:", targetId);
    }
    return false; // Prevent default action again for good measure
  }
  
  // Verify sections exist
  const sections = ['jaunumi', 'contact-section', 'parbiedribu'];
  sections.forEach(id => {
    const section = document.getElementById(id);
    console.log(`Section '${id}' exists:`, !!section);
  });
});
</script>



  
  





